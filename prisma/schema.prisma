generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Fornecedor {
  id              String @id @default(auto()) @map("_id") @db.ObjectId
  nome            String
  logo            String
  estadoOrigem    String
  custoPorKwh     Float
  limiteMinimoKwh Int
  totalClientes   Int
  avaliacaoMedia  Float
}
